class biller:

def __init__(self):

pass

def PRINT_BILL(self, res):
print(res)

def ADD_ITEM(*args):

d = {TSHIRT: [1000, 10], 'JACKET': [2000, 5], 'CAP': [500, 20], 'NOTEBOOK': [200, 20], 'PENS': [300, 10], 'MARKERS': [500, 5]} if args[0] in ['TSHIRT', 'JACKET', 'CAP']:

if int(args[-1]) > 2:


return "ERROR QUANTITY_EXCEEDED"


val = d[args[0]]

price = int(args[-1]) * val[0]


price (100-val[-1])/100 * price

return price

if int(args[-1]) 3:
return "ERROR QUANTITY_EXCEEDED"
